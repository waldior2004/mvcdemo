@model com.msc.infraestructure.entities.Banco
@{ 
    Layout = null;
    <div class="row">
        @Html.LabelFor(m => m.Id, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Id</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.IdPais, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Pais.Descripcion</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Codigo, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Codigo</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Descripcion, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Descripcion</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Direccion, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Direccion</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Swift, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Swift</p>
        </div>
    </div>
}
