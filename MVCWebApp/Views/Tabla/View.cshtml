@model com.msc.infraestructure.entities.Tabla
@{ 
    Layout = null;
    <div class="row">
        @Html.LabelFor(m => m.Id, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Id</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.IdGrupoTabla, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.GrupoTabla.Descripcion</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Codigo, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Codigo</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Orden, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Orden</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Descripcion, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Descripcion</p>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(m => m.Breve, new { @class = "col-md-3 control-label" })
        <div class="col-md-8">
            <p>@Model.Breve</p>
        </div>
    </div>
}
